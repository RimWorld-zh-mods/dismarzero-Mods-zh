<?xml version="1.0" encoding="utf-8" ?>
<Plants>


	

	

  <ThingDef ParentName="VG_PlantDef">
    <defName>PlantSteelLeaf</defName>
    <label>steel shroom tree</label>
    <description>Plants with natural occurring steel in it's viens. Steel shrooms can be smelted at an Electric Smelter into steel ore.</description>
    <graphicData>
      <texPath>Things/Plants/SteelPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <plant>
      <harvestedThingDef>RawSteelLeaf</harvestedThingDef>
      <harvestYield>16</harvestYield>
      <sowMinSkill>8</sowMinSkill>
      <growDays>12</growDays>
      <fertilitySensitivity>1</fertilitySensitivity>
      <sowResearchPrerequisites>
        <li>resourceleafs</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>

