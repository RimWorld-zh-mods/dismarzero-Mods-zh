<RecipeDef>
      <defName>CookGrillcheese</defName>
      <label>cook two grilled cheeses</label>
      <description>Two slices of bread grilled with cheese melt in the middle. \n\nMakes 2 servings. \n\nFood Effects: Metabolism.</description>
      <jobString>Toasting bread.</jobString>  
      <allowMixingIngredients>true</allowMixingIngredients>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Bread</li>
               </thingDefs>
            </filter>
            <count>4</count>
         </li>
		<li>
            <filter>
               <thingDefs>
                  <li>cheese</li>
               </thingDefs>
            </filter>
            <count>2</count>
         </li>
      </ingredients>
      <products>
         <grilledcheese>2</grilledcheese>
      </products>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Bread</li>
		<li>cheese</li>
         </thingDefs>
      <disallowedSpecialFilters>
        <li>AllowRotten</li>
      </disallowedSpecialFilters>
      </fixedIngredientFilter>
      <workAmount>1000</workAmount>
      <workSpeedStat>CookSpeed</workSpeedStat>
      <workSkill>Cooking</workSkill>
      <effectWorking>Cook</effectWorking>
   	 <soundWorking>Recipe_CookMeal</soundWorking>
	 <recipeUsers>
         <li>ElectricStove</li>
	    <li>FueledStove</li>
      </recipeUsers>
      </RecipeDef>

