<?xml version="1.0" encoding="utf-8" ?>
<Defs>




<RecipeDef>
	<defName>Fruitdrink</defName>
	<label>make fruit drink</label>
	<description>Press fruit into a juicy drink.</description>
	<jobString>Pressing fruit.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
     <soundWorking>Recipe_CookMeal</soundWorking>
     <allowMixingIngredients>true</allowMixingIngredients>
	<recipeUsers>
         <li>Brewery</li>
      </recipeUsers>
	<ingredients>
	  <li>
		<filter>
			<categories>
					<li>FruitFoodRaw</li>
			</categories>
		</filter>
		<count>20</count>
	   </li>

	</ingredients>
	<fixedIngredientFilter>
	  	<categories>
			<li>FruitFoodRaw</li>
		</categories>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
	</fixedIngredientFilter>
	<products>
	  <Fruitdrink>4</Fruitdrink>
	</products>
	<workSkill>Cooking</workSkill>
</RecipeDef>



<RecipeDef>
	<defName>MakeChocolatemilk</defName>
	<label>make chocomilk</label>
	<description>Blend chocolate and milk.</description>
	<jobString>Mixing milk.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
<workAmount>800</workAmount>
    <soundWorking>Recipe_CookMeal</soundWorking>          
	<allowMixingIngredients>true</allowMixingIngredients>
	<ingredients>
	  <li>
	    <filter>
	      <thingDefs>
	        <li>Chocolate</li>
	      </thingDefs>
	    </filter>
	    <count>5</count>
	  </li>
	<li>
	    <filter>
	      <thingDefs>
	        <li>soymilk</li>
		   <li>Milk</li>
	        <li>coconutmilk</li>				
	      </thingDefs>
	    </filter>
	    <count>5</count>
	  </li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
	    <li>Chocolate</li>
	    <li>soymilk</li> 
	    <li>Milk</li>
          <li>coconutmilk</li>				
	  </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>

	</fixedIngredientFilter>
	<products>
	  <Chocomilk>4</Chocomilk>
	</products>
	<recipeUsers>
         <li>Brewery</li>
      </recipeUsers>
	<workSkill>Cooking</workSkill>
</RecipeDef>





</Defs>